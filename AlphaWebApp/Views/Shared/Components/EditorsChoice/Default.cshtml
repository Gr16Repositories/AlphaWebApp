@model IEnumerable<AlphaWebApp.Models.Article>

<!--Editor Choice-->
@foreach (var article in Model)
{
    <div>
        <a asp-controller="Articles" asp-action="ContinueReading" asp-route-id="@article.Id" class="headline" style="color:black">
            <img src="@article.ImageLink" class="d-block w-100" alt="img" style="height: 200px; width:100%">
            <h4>
                @article.HeadLine
            </h4>
        </a>
        @*@article.HeadLine*@
    </div>
}
